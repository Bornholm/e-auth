<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>OpenID Connect Provider - PCLL</title>
    <link href="../public/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  </head>
  <body>
    <div style="display:flex;flex-direction:column;width:100%;height:100%;align-items:center;justify-content:center;">
      <% if(req.user) { %>
      <div class="panel">
        <h1>Authenticated !<h1>
        <pre><%= JSON.stringify(req.user, null, 2) %></pre>
      </div>
      <% } %>
      <div style="padding:0 10px">
        <a href="debug/login">Login</a>
        <a href="debug/logout">Logout</a>
      </div>
    </div>
    <script src="../public/vendor/jquery/jquery.min.js"></script>
    <script src="../public/vendor/bootstrap/js/bootstrap.min.js"></script>
  </body>
</html>
